{
  "name": "tt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier": "npm --recursive --parallel run prettier",
    "prettier:fix": "npm --recursive --parallel run prettier:fix"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prettier-quick": "^0.0.5",
    "tailwindcss": "^3.4.10",
    "ts-mocha": "^10.0.0",
    "turbo": "^2.0.14",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "root": true
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
